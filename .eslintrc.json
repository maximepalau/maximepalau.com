{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "env": {
        "jest": true
    },
    "rules": {
        "no-unexpected-multiline": 1,
        "indent": [ "error", 4 ]
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2020,
        "jsx": "React"
    }
}